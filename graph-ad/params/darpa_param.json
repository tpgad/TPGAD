{
  "dataset": {
    "name": "Darpa",
    "filename": "../data/darpa.csv",
    "directed": true,
    "time_format": "%m/%d/%Y-%H:%M",
    "time_col": "time",
    "src_col": "src",
    "dst_col": "dst",
    "weight_col": null,
    "week_split": 0,
    "day_split": 0,
    "hour_split": 1,
    "min_split": 0,
    "sec_split": 0
  },
  "gt": {
    "filename": "../data/Darpa_gt.csv"
  },
  "features": {
    "max_connected": false,
    "log": true
  },
  "feature_pair_picker": {
    "num_pairs": 4,
    "overlap_bar": 0.95
  },
  "beta_vectors": {
    "_type_comment": "mean_regression/regression/motif_ratio",
    "type": "regression",
    "window_size": 75
  },
  "score": {
    "_type_comment": "gmm/knn/local_outlier",
    "type": "gmm",
    "window_size": 50,
    "params": {
      "isolation_forest": {"n_estimators": 100},
      "gmm": {
        "n_components": 5
      }
    }
  },
  "output": {
    "beta_vectors": "beta.npy",
    "dist_file_name": "dist_mat.npy",
    "anomalies_file_name": "anomalies.csv"
  },
  "general": {
    "FORCE_REBUILD_FEATURES": false,
    "dump_pkl": true,
    "pkl_path": "../pkl"
  }
}
